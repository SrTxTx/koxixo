{
  "name": "koxixo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "node build-vercel.js",
    "postinstall": "prisma generate",
    "clean": "rd /s /q .next node_modules 2>nul && del package-lock.json 2>nul && npm cache clean --force && npm install && npx prisma generate",
    "reset": "taskkill /f /im node.exe 2>nul && npm run clean",
    "fresh-start": "npm run clean:cache && npm install && npm run dev",
    "clean:cache": "rd /s /q .next 2>nul & rd /s /q node_modules\\.cache 2>nul & npm cache clean --force"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "bcryptjs": "^2.4.3",
    "lucide-react": "0.292.0",
    "next": "^14.2.10",
    "next-auth": "^4.24.11",
    "pg": "^8.16.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "2.8.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "20.11.30",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "postcss": "8.4.38",
    "prisma": "5.22.0",
    "tailwindcss": "3.4.1",
    "tsx": "^4.20.6",
    "typescript": "5.4.2"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  }
}
